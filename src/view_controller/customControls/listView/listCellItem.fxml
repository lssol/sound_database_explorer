<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hBox" alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <ImageView fx:id="image" fitHeight="20.0"  onMouseClicked="#play" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image  url="@../../../ressources/play.png" />
         </image>
      </ImageView>
        <Label fx:id="name" text="Name">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin></Label>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
